    "pid": "0x0899",
    "device_version": "1.0.0"
  },
  "processor": "RP2040",
  "bootloader": "rp2040",
  "features": {
    "extrakey": true,
    "pointing_device": true,
    "oled": true,
    "rgblight": true,
    "midi": true
  },
  "bootmagic": {
    "enabled": true,
    "matrix": [0, 1]
  },
  "mouse_key": {
    "enabled": true
  },
  "matrix_pins": {
    "cols": ["GP3", "GP4", "GP5", "GP6", "GP7", "GP11", "GP12"],
    "rows": ["GP3", "GP4", "GP5", "GP6", "GP7", "GP11", "GP12"]
  },
  "diode_direction": "COL2ROW",
  "dynamic_keymap": {
    "layer_count": 11
  },
  "encoder": {
    "enabled": true,
    "rotary": [
      { "pin_a": "GP13", "pin_b": "GP14", "resolution": 2 },
      { "pin_a": "GP15", "pin_b": "GP17", "resolution": 2 },
      { "pin_a": "GP22", "pin_b": "GP26", "resolution": 2 },
      { "pin_a": "GP20", "pin_b": "GP21", "resolution": 2 }
    ]
  },
  "split": {
    "enabled": true,
    "soft_serial_pin": "GP1",
    "transport": {
      "protocol": "serial",
      "sync_matrix_state": true
    },
    "bootmagic": {
      "matrix": [7, 1]
    },
    "usb_detect": {
      "enabled": true,
      "polling_interval": 25,
      "timeout": 3000
    }
  },
  "rgblight": {
    "led_count": 66,
    "split": true,
    "split_count": [33, 33],
    "max_brightness": 200,
    "hue_steps": 4,
    "saturation_steps": 4,
    "brightness_steps": 8,
    "animations": {
      "alternating": true,
      "breathing": true,
      "christmas": true,
      "knight": true,
      "rainbow_mood": true,
      "rainbow_swirl": true,
      "rgb_test": true,
      "snake": true,
      "static_gradient": true,
      "twinkle": true
    },
    "sleep": false,
    "layers": {
      "enabled": true,
      "max": 6
    }
  },
  "ws2812": {
    "pin": "GP2",
    "driver": "vendor"
